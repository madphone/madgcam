<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen android:orderingFromXml="true" android:key="prefscreen_top"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <PreferenceCategory android:title="@string/setting_general" android:key="pref_category_general">
        <PreferenceScreen android:layout="@layout/preference_with_margin" android:title="@string/pref_bsg_mod_title" android:key="pref_bsg_mod_key">
            <ListPreference android:entries="@array/pref_input_model_entries" android:layout="@layout/preference_with_margin" android:title="@string/pref_input_model_title" android:key="pref_input_model_key" android:summary="@string/pref_all_title_summary" android:defaultValue="@string/pref_input_model_default" android:entryValues="@array/pref_input_model_entryvalues" />
            <com.google.android.apps.camera.legacy.app.settings.ManagedSwitchPreference android:layout="@layout/preference_with_margin" android:title="@string/pref_oneconf_title" android:key="pref_oneconf_key" android:defaultValue="false" />
            <ListPreference android:entries="@array/pref_parameters_camera_entries" android:layout="@layout/preference_with_margin" android:title="@string/pref_parameters_camera_title" android:key="pref_parameters_camera_key" android:summary="@string/pref_all_title_summary" android:defaultValue="@string/pref_parameters_camera_default" android:entryValues="@array/pref_parameters_camera_entryvalues" />
            <PreferenceScreen android:layout="@layout/preference_with_margin" android:title="@string/pref_sblp_title" android:key="pref_sblp_key">
                <com.google.android.apps.camera.legacy.app.settings.ManagedSwitchPreference android:layout="@layout/preference_with_margin" android:title="@string/pref_enable_sblp_title" android:key="pref_enable_sblp_key" android:summary="@string/pref_all_title_summary_sblp" android:defaultValue="false" />
                <ListPreference android:entries="@array/pref_sblp_entries" android:layout="@layout/preference_with_margin" android:title="@string/setting_dg_title" android:key="pref_dg_key" android:summary="%s" android:defaultValue="@string/pref_sblp_default" android:entryValues="@array/pref_sblp_entryvalues" />
                <ListPreference android:entries="@array/pref_sblp_entries" android:layout="@layout/preference_with_margin" android:title="@string/setting_r_title" android:key="pref_r_key" android:summary="%s" android:defaultValue="@string/pref_sblp_default" android:entryValues="@array/pref_sblp_entryvalues" />
                <ListPreference android:entries="@array/pref_sblp_entries" android:layout="@layout/preference_with_margin" android:title="@string/setting_lg_title" android:key="pref_lg_key" android:summary="%s" android:defaultValue="@string/pref_sblp_default" android:entryValues="@array/pref_sblp_entryvalues" />
                <ListPreference android:entries="@array/pref_sblp_entries" android:layout="@layout/preference_with_margin" android:title="@string/setting_b_title" android:key="pref_b_key" android:summary="%s" android:defaultValue="@string/pref_sblp_default" android:entryValues="@array/pref_sblp_entryvalues" />
            </PreferenceScreen>
            <ListPreference android:entries="@array/pref_qjpg_entries" android:layout="@layout/preference_with_margin" android:title="@string/pref_qjpg_title" android:key="pref_qjpg_key" android:summary="%s" android:defaultValue="@string/pref_qjpg_default" android:entryValues="@array/pref_qjpg_entryvalues" />
            <com.google.android.apps.camera.legacy.app.settings.ManagedSwitchPreference android:layout="@layout/preference_with_margin" android:title="@string/pref_compress_dng_title" android:key="pref_compress_dng_key" android:defaultValue="true" />
            <com.google.android.apps.camera.legacy.app.settings.ManagedSwitchPreference android:layout="@layout/preference_with_margin" android:title="@string/pref_pmode_title" android:key="pref_pmode_key" android:summary="@string/pref_all_title_summary_pmode" android:defaultValue="false" />
            <com.google.android.apps.camera.legacy.app.settings.ManagedSwitchPreference android:layout="@layout/preference_with_margin" android:title="@string/pref_sbe_title" android:key="pref_sbe_key" android:summary="@string/pref_all_title_summary_sbe" android:defaultValue="false" />
        </PreferenceScreen>
        <com.google.android.apps.camera.legacy.app.settings.ManagedSwitchPreference android:layout="@layout/preference_with_margin" android:title="@string/pref_camera_save_location_title" android:key="pref_camera_recordlocation_key" android:order="0" android:defaultValue="false" />
        <com.google.android.apps.camera.legacy.app.settings.ManagedSwitchPreference android:layout="@layout/preference_with_margin" android:title="@string/pref_camera_sounds_title" android:key="pref_camera_sounds_key" android:order="1" android:defaultValue="true" />
        <PreferenceScreen android:layout="@layout/preference_with_margin" android:title="@string/pref_camera_gestures_title" android:key="pref_category_gestures" android:order="10">
            <ListPreference android:entries="@array/pref_volume_action_entries" android:layout="@layout/preference_with_margin" android:title="@string/pref_camera_volume_key_action_title" android:key="pref_volume_key_action" android:summary="%s" android:defaultValue="@string/volume_key_shutter" android:entryValues="@array/pref_volume_action_entryvalues" />
            <com.google.android.apps.camera.legacy.app.settings.ManagedSwitchPreference android:layout="@layout/preference_with_margin" android:title="@string/pref_camera_double_tap_zoom_title" android:key="pref_double_tap_zoom_key" android:defaultValue="true" />
        </PreferenceScreen>
    </PreferenceCategory>
    <PreferenceCategory android:title="@string/photo" android:key="pref_category_resolution_camera">
        <ListPreference android:layout="@layout/preference_with_margin" android:title="@string/setting_back_camera_photo" android:key="pref_camera_picturesize_back_key" android:summary="%s" android:defaultValue="@string/pref_camera_picturesize_default" android:entryValues="@array/pref_camera_picturesize_entryvalues" />
        <ListPreference android:layout="@layout/preference_with_margin" android:title="@string/setting_front_camera_photo" android:key="pref_camera_picturesize_front_key" android:summary="%s" android:defaultValue="@string/pref_camera_picturesize_default" android:entryValues="@array/pref_camera_picturesize_entryvalues" />
        <ListPreference android:entries="@array/pref_refocusmb_entries" android:layout="@layout/preference_with_margin" android:title="@string/pref_refocusmb_title" android:key="pref_refocusmb_key" android:summary="@string/pref_refocusmb_summary" android:defaultValue="@string/pref_refocusmb_default" android:entryValues="@array/pref_refocusmb_entryvalues" />
        <PreferenceScreen android:layout="@layout/preference_with_margin" android:title="@string/pref_burst_settings" android:key="pref_category_smartburst">
            <com.google.android.apps.camera.legacy.app.settings.ManagedSwitchPreference android:layout="@layout/preference_with_margin" android:title="@string/pref_burst_title" android:key="pref_smartburst_classicburst" android:summary="@string/pref_smartburst_summary" android:defaultValue="true" />
            <com.google.android.apps.camera.legacy.app.settings.ManagedSwitchPreference android:layout="@layout/preference_with_margin" android:title="@string/pref_auto_generate_artifacts_toggle" android:key="pref_auto_generate_artifacts" android:defaultValue="true" />
        </PreferenceScreen>
        <PreferenceScreen android:layout="@layout/preference_with_margin" android:title="@string/pref_category_advanced" android:key="pref_category_advanced">
            <com.google.android.apps.camera.legacy.app.settings.ManagedSwitchPreference android:layout="@layout/preference_with_margin" android:title="@string/pref_camera_dirty_lens_title" android:key="pref_camera_dirty_lens_key" android:defaultValue="true" />
            <com.google.android.apps.camera.legacy.app.settings.ManagedSwitchPreference android:layout="@layout/preference_with_margin" android:title="@string/pref_hdr_plus_control_title" android:key="pref_camera_hdrplus_option_available_key" android:summary="@string/pref_hdr_plus_control_summary" android:defaultValue="false" />
        </PreferenceScreen>
        <PreferenceScreen android:layout="@layout/preference_with_margin" android:title="@string/pref_experimental" android:key="pref_category_experimental">
            <com.google.android.apps.camera.legacy.app.settings.ManagedSwitchPreference android:layout="@layout/preference_with_margin" android:title="@string/pref_gcam_hdr_raw_title" android:key="pref_camera_hdr_plus_raw_out_key" android:summary="@string/pref_gcam_hdr_raw_summary" android:defaultValue="false" />
        </PreferenceScreen>
    </PreferenceCategory>
    <PreferenceCategory android:title="@string/mode_video" android:key="pref_category_resolution_video">
        <ListPreference android:layout="@layout/preference_with_margin" android:title="@string/setting_back_camera_video" android:key="pref_video_quality_back_key" android:summary="%s" android:defaultValue="@string/pref_video_quality_large" android:entryValues="@array/pref_video_quality_entryvalues" />
        <ListPreference android:layout="@layout/preference_with_margin" android:title="@string/setting_front_camera_video" android:key="pref_video_quality_front_key" android:summary="%s" android:defaultValue="@string/pref_video_quality_large" android:entryValues="@array/pref_video_quality_entryvalues" />
        <com.google.android.apps.camera.legacy.app.settings.ManagedSwitchPreference android:layout="@layout/preference_with_margin" android:title="@string/pref_video_stabilization" android:key="pref_video_stabilization_key" android:defaultValue="true" />
    </PreferenceCategory>
    <PreferenceCategory android:title="@string/setting_google_help_and_feedback" android:key="pref_category_help_and_feedback">
        <Preference android:enabled="false" android:layout="@layout/preference_with_margin" android:title="@string/setting_google_feedback" android:key="pref_launch_feedback" />
        <Preference android:enabled="false" android:layout="@layout/preference_with_margin" android:title="@string/setting_google_help" android:key="pref_launch_help" />
        <PreferenceScreen android:layout="@layout/preference_with_margin" android:title="@string/pref_developer_settings" android:key="pref_category_developer" />
    </PreferenceCategory>
</PreferenceScreen>